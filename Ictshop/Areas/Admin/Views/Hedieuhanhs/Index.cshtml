@model IEnumerable<Ictshop.Models.Hedieuhanh>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Quản lý hệ điều hành</h2>

<p>
    @Html.ActionLink("Create new", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Code
        </th>
        <th>
            Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Mahdh)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Tenhdh)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Mahdh }) |
        @Html.ActionLink("Details", "Details", new { id = item.Mahdh }) |
        @Html.ActionLink("Delete ", "Delete", new { id = item.Mahdh })
    </td>
</tr>
}

</table>
